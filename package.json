{
  "name": "example",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "npm run dll && npm run serve",
    "build": "webpack",
    "serve": "webpack -w",
    "production": "webpack -p --env=production",
    "test": "karma start --single-run",
    "dll": "webpack --config webpack.dll.config.js",
    "map": "source-map-explorer public/assets/js/main.js public/assets/js/main.js.map",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor -p protractor.conf.js",
    "lint": "tslint 'src/**/*.ts' && stylelint 'src/**/*.css'"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.1",
    "@angular/compiler-cli": "^4.0.0",
    "@frontpack/frontpack": "3.0.0-beta.2",
    "@frontpack/frontpack-preset-angular": "3.0.0-beta.2",
    "@frontpack/frontpack-preset-common": "3.0.0-beta.2",
    "@frontpack/frontpack-preset-copy": "3.0.0-beta.2",
    "@frontpack/frontpack-preset-dll": "3.0.0-beta.2",
    "@frontpack/frontpack-preset-html": "3.0.0-beta.2",
    "@frontpack/frontpack-preset-server": "3.0.0-beta.2",
    "@frontpack/frontpack-preset-style": "3.0.0-beta.2",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "source-map-explorer": "^1.3.3",
    "stylelint": "^7.10.0",
    "stylelint-config-standard": "^16.0.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "webpack": "^2.4.1"
  }
}
